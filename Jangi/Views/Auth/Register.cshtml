@model Jangi.ViewModels.AuthRegister

@{ 
    ViewBag.Title = "Register";
}
@using (Html.BeginForm())
{
    @Html.ValidationSummary()

    <div class="form-group">
        @Html.LabelFor(x => 
            x.pseudo, "Pseudo")
        @Html.EditorFor(x =>
            x.pseudo)
    </div>
    <div class="form-group">
        @Html.LabelFor(x => 
            x.email, "Email")
        @Html.EditorFor(x =>
            x.email)
    </div>
    <div class="form-group">
        @Html.LabelFor(x =>
            x.birthDate,
            "Date de Naissance")
        @Html.EditorFor(x => 
            x.birthDate)
    </div>
    <div class="form-group">
        @Html.LabelFor(x => 
            x.password,
            "Mot de Passe")
        @Html.EditorFor(x =>
            x.password)
    </div>
    <div class="form-group">
        <input type="submit"
         value="S'inscrire" 
             title="blalba"/>
    </div>
}
